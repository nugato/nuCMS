{% block subject %}
    {{ 'nucms.web.form.contact_form.subject'|trans }}
{% endblock %}

{% block body %}
    <b>{{ 'nucms.web.form.contact_form.name'|trans }}</b>: {{ name }} <br>
    <b>{{ 'nucms.web.form.contact_form.phone'|trans }}</b>: {{ phone }} <br>
    <b>{{ 'nucms.web.form.contact_form.email'|trans }}</b>: {{ email }} <br>
    <b>{{ 'nucms.web.form.contact_form.message'|trans }}</b>: <br>{{ message }}
{% endblock %}
